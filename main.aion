type Animal {
    name: String
    age: Int

    constructor(name: String) {
        it.name = name
    }

    abstract fn bark() -> String 
}

type Dog: Animal {
    fn bark() -> String {
        return "Woof!"
    }
}

fn makeSound(animal: Animal) -> String {
    return animal.bark()
}

fn main() {
    
    println(makeSound(Dog("Fido")))
}